{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "job-scraper-api",
  "main": "src/index.ts",
  "compatibility_date": "2026-02-25",
  "assets": {
    "directory": "./dist",
    "binding": "ASSETS"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "job-scraper-db",
      "database_id": "<YOUR_D1_ID>",
      "migrations_dir": "drizzle"
    }
  ],
  "ai": {
    "binding": "AI"
  },
  "observability": {
    "enabled": true
  }
}
